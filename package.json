{
  "name": "@rowno/http-stub",
  "version": "1.2.0",
  "description": "Simple HTTP stubbing library for Node.js",
  "author": "Roland Warmerdam (https://roland.codes)",
  "keywords": [
    "http",
    "mock",
    "stub",
    "node"
  ],
  "repository": "Rowno/http-stub",
  "license": "MIT",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "ava",
    "lint": "eslint '**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "joi": "^14.3.0",
    "micro": "^9.3.3",
    "openssl-self-signed-certificate": "^1.1.6"
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.12.0",
    "got": "^9.4.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "prettier": "^2.1.2",
    "selfsigned": "^1.10.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "es2017": true,
      "node": true
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}

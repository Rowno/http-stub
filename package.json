{
  "name": "@rowno/http-stub",
  "version": "1.1.0",
  "description": "Simple HTTP stubbing library for Node.js",
  "author": "Roland Warmerdam (https://roland.codes)",
  "keywords": [
    "http",
    "mock",
    "stub",
    "node"
  ],
  "repository": "Rowno/http-stub",
  "license": "MIT",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "xo && ava"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=8.10"
  },
  "dependencies": {
    "joi": "^14.3.0",
    "micro": "^9.3.3",
    "openssl-self-signed-certificate": "^1.1.6"
  },
  "devDependencies": {
    "ava": "^1.0.1",
    "got": "^9.4.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "selfsigned": "^1.10.4",
    "xo": "^0.23.0"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "semicolon": false
  },
  "lint-staged": {
    "*.js": [
      "xo --fix",
      "git add"
    ]
  }
}
